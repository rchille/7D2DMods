		<item name="Rchille_stoveElectricSchematic">
        		<property name="Extends" value="schematicMaster"/>
		        <!-- property name="DescriptionKey" value="schematicMaster2Desc"/> This would have to be added to the Localizations-->
		        <property name="EconomicValue" value="500"/>
